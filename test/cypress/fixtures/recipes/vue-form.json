            "Template": {
              "Text": "<v-container>\r\n    <v-card-text>\r\n        <v-row justify=\"center\">\r\n            <v-col cols=\"8\">\r\n                <validation-provider name=\"Bag\" rules=\"required\" v-slot=\"{ errors, valid }\">\r\n                    <bag v-model=\"bagValues\" :valueNames=\"valueNames\" :valid=\"valid\"\r\n                        :error-messages=\"errors\" add-button-label=\"Add\" remove-button-label=\"-\">\r\n\r\n                        <template #validations>\r\n                            <validation-provider rules=\"required\" name=\"Field 1\"></validation-provider>\r\n                            <validation-provider rules=\"required\" name=\"Field 2\"></validation-provider>\r\n                        </template>\r\n\r\n\r\n                        <template #components>\r\n                            <v-text-field filled=\"filled\" label=\"label\">\r\n                            </v-text-field>\r\n                            <v-text-field filled=\"filled\" label=\"label\">\r\n                            </v-text-field>\r\n                        </template>\r\n\r\n                        <template #title>\r\n                            <div class=\"text-h5 mb-5\">\r\n                                Bag\r\n                            </div>\r\n                        </template>\r\n                    </bag>\r\n                </validation-provider>\r\n            </v-col>\r\n            <v-col cols=\"8\">\r\n                <v-alert type=\"success\" v-if=\"form.successMessage\">\r\n                    {% raw %}{{ form.successMessage }}{% endraw %}\r\n                </v-alert>\r\n                <v-alert type=\"error\" v-if=\"form.serverValidationMessage\">\r\n                    {% raw %}{{ form.serverValidationMessage[0] }}{% endraw %}\r\n                </v-alert>\r\n            </v-col>\r\n            <v-col cols=\"8\">\r\n                <v-btn type=\"submit\" depressed block @click=\"formHandleSubmit\" :disabled=\"form.submitting\">{{\r\n                    \"submitLabel\" | localize }}</v-btn>\r\n            </v-col>\r\n        </v-row>\r\n    </v-card-text>\r\n</v-container>"
