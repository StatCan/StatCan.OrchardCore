{% assign menuItem = Model.Properties.Model %}

<v-list-item href="{{ menuItem.Content.LinkMenuItemPart.Url | href  }}">
  <v-list-item-icon>
    <v-icon color="black">{{ menuItem.Content.CommonMenuItemPart.IconName.Text }}</v-icon>
  </v-list-item-icon>
  <v-list-item-content>
    <v-list-item-title>
      <span class="font-weight-bold">
          {{ menuItem.Content.LinkMenuItemPart.Name }}
      </span>
    </v-list-item-title>
  </v-list-item-content>
</v-list-item>
