{% assign menuItem = Model.Properties.Model %}
<v-list-item href="{{ menuItem.LinkMenuItemPart.Url | href }}">
  <v-list-item-icon>
    <v-icon color="black">{{ menuItem.CommonMenuItemPart.IconName.Text }}</v-icon>
  </v-list-item-icon>
  <v-list-item-content>
    <v-list-item-title>
      <span class="font-weight-bold">
          {{ menuItem.LinkMenuItemPart.Name }}
      </span>
    </v-list-item-title>
  </v-list-item-content>
</v-list-item>
