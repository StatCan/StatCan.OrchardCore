{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "OrchardCore.Workflows": "Trace"
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel":  "Error"
    }
  },
  "IdentityOptions": {
    "Password": {
      "RequireDigit": false,
      "RequireLowercase": false,
      "RequireUppercase": false,
      "RequireNonAlphanumeric": false,
      "RequiredUniqueChars": 1,
      "RequiredLength": 2
    }
  }
}