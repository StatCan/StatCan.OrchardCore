{% assign social = Model.ContentItem.Content.SocialLinks %}
<ul class="navbar-nav flex-row">
  {% if social.Email.Url != null %}
  <li class="nav-item" data-toggle="tooltip" data-placement="top" title="{{ social.Email.Text }}">
    <a class="social-link nav-link" href="{{ social.Email.Url }}">
      <i class="fas fa-at fa-secondary mx-2" 
         aria-label="{{ social.Email.Text }}"></i>
    </a>
  </li>
  {% endif %}
  {% if social.GCcollab.Url != null %}
  <li class="nav-item" data-toggle="tooltip" data-placement="top" title="{{ social.GCcollab.Text }}">
    <a class="social-link nav-link" href="{{ social.GCcollab.Url }}">
      <svg class="mx-2 mb-1">
        <use xlink:href="#gccollab" />
      </svg>
    </a>
  </li>
  {% endif %}
  {% if social.Slack.Url != null %}
  <li class="nav-item" data-toggle="tooltip" data-placement="top" title="{{ social.Slack.Text }}">
    <a class="social-link nav-link" href="{{ social.Slack.Url }}">
      <i class="fab fa-slack fa-secondary mx-2" 
         aria-label="{{ social.Slack.Text }}"></i>
    </a>
  </li>
  {% endif %}
  {% if social.Twitter.Url != null %}
  <li class="nav-item" data-toggle="tooltip" data-placement="top" title="{{ social.Twitter.Text }}">
    <a class="social-link nav-link" href="{{ social.Twitter.Url }}">
      <i class="fab fa-twitter fa-secondary mx-2" 
         aria-label="{{ social.Twitter.Text }}"></i>
    </a>
  </li>
  {% endif %}
</ul>

