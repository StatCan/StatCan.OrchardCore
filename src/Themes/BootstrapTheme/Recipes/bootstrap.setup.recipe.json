{
  "name": "bootstrap-theme-setup",
  "displayName": "Bootstrap Theme Setup",
  "description": "Good starting point, bootstrap theme recipe",
  "author": "Digital Innovation Team",
  "website": "https://digital.statcan.gc.ca",
  "version": "1.0",
  "issetuprecipe": true,
  "categories": ["bootstrap"],
  "tags": ["bootstrap", "bs4"],
  "steps": [
      {
        "name": "feature",
        "disable": [],
        "enable": [
          "OrchardCore.HomeRoute",
          "OrchardCore.Admin",
          "OrchardCore.Diagnostics",
          "OrchardCore.DynamicCache",
          "OrchardCore.Features",
          "OrchardCore.Navigation",
          "OrchardCore.Recipes",
          "OrchardCore.Resources",
          "OrchardCore.Roles",
          "OrchardCore.Settings",
          "OrchardCore.Themes",
          "OrchardCore.Users",
          "OrchardCore.Alias",
          "OrchardCore.Autoroute",
          "OrchardCore.Html",
          "OrchardCore.ContentFields",
          "OrchardCore.ContentLocalization",
          "OrchardCore.ContentLocalization.ContentCulturePicker",
          "OrchardCore.ContentPreview",
          "OrchardCore.Contents",
          "OrchardCore.ContentTypes",
          "OrchardCore.CustomSettings",
          "OrchardCore.Deployment",
          "OrchardCore.Feeds",
          "OrchardCore.Indexing",
          "OrchardCore.Layers",
          "OrchardCore.Localization",
          "OrchardCore.Lists",
          "OrchardCore.Liquid",
          "OrchardCore.Markdown",
          "OrchardCore.Media",
          "OrchardCore.Menu",
          "OrchardCore.Queries",
          "OrchardCore.Title",
          "OrchardCore.Templates",
          "OrchardCore.Widgets",
          "OrchardCore.Email",
          "OrchardCore.Flows",
  
          "StatCan.OrchardCore.DisplayHelpers",
          "DigitalAdmin",
          "BootstrapTheme"
        ]
      },
      {
        "name": "Settings",
        "Calendar": null,
        "UseCdn": false,
        "TimeZoneId": "America/Toronto",
        "AppendVersion": true,
        "LocalizationSettings": {
          "DefaultCulture": "en",
          "SupportedCultures": ["fr", "en"]
        },
        "LoginSettings": {
          "UseSiteTheme": true
        },
        "ResetPasswordSettings": {
          "UseSiteTheme": true
        },
        "RegistrationSettings": {
          "UseSiteTheme": true
        }
      },
      {
        "name": "themes",
        "admin": "DigitalAdmin",
        "site": "BootstrapTheme"
      }
  ]
}
