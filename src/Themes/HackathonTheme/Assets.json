[
  {
    "inputs": ["Assets/scss/hackathon.scss"],
    "watch": [
      "Assets/scss/_bootstrap.scss",
      "Assets/scss/_join-team-widget.scss",
      "Assets/scss/_variables.scss"
    ],
    "output": "wwwroot/css/hackathon.css"
  },
  {
    "inputs": ["node_modules/jquery/dist/jquery.js"],
    "output": "wwwroot/Scripts/jquery.js"
  },
  {
    "inputs": ["node_modules/bootstrap/dist/js/bootstrap.js"],
    "output": "wwwroot/Scripts/bootstrap.js"
  },
  {
    "inputs": ["node_modules/popper.js/dist/umd/popper.js"],
    "output": "wwwroot/Scripts/popper.js"
  },
  {
    "copy": true,
    "inputs": ["node_modules/@fortawesome/fontawesome-free/css/*"],
    "output": "wwwroot/Vendor/fontawesome-free/css/@"
  },
  {
    "copy": true,
    "inputs": ["node_modules/@fortawesome/fontawesome-free/js/*"],
    "output": "wwwroot/Vendor/fontawesome-free/js/@"
  },
  {
    "copy": true,
    "inputs": ["node_modules/@fortawesome/fontawesome-free/webfonts/*"],
    "output": "wwwroot/Vendor/fontawesome-free/webfonts/@"
  },
  {
    "inputs": ["Assets/js/setCookie.js", "Assets/js/header.js"],
    "output": "wwwroot/Scripts/hackathon.js"
  }
]
