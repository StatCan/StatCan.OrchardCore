<!DOCTYPE html>
<html lang="{{ Culture.Name }}">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    {% render_section "HeadMeta", required: false %}
    <title>{% page_title Site.SiteName %}</title>
    {% link src:"/HackathonTheme/favicon-32x32.png", rel:"icon", sizes:"32x32", type:"image/png" %}
    {% link src:"/HackathonTheme/favicon-16x16.png", rel:"icon", sizes:"16x16", type:"image/png" %}
    {% link src:"/apple-touch-icon.png", rel:"apple-touch-icon", sizes:"180x180" %}
    {% link src:"/favicon-194x194.png", rel:"icon", type:"image/png", sizes:"194x194" %}
    {% link src:"/android-chrome-192x192.png", rel:"icon", type:"image/png", sizes:"192x192" %}
    {% link src:"/safari-pinned-tab.svg", rel:"mask-icon", color:"#101010" %}

    {% resources type: "Meta" %}

    <!-- Includes customized Bootstrap CSS -->
    {% style name:"portaltheme-stc-bootstrap", version:"1" %}

    <!-- Custom fonts for this template -->
    {% style name:"font-awesome", version:"5" %}
    {% style src:"https://fonts.googleapis.com/css2?family=Fira+Sans:wght@200;300;400;500;700;800&display=swap" %}

    <!-- Bootstrap core JavaScript (includes jQuery and popper.js) -->
    {% script name:"jQuery", at:"Foot", version:"3.4.1" %}
    {% script name:"popper", at:"Foot", version:"1.16.1" %}
    {% script name:"bootstrap", at:"Foot", version:"4.4.1" %}
    {% script src:"~/PortalTheme/Scripts/setCookie.min.js", debug_src:"~/PortalTheme/Scripts/setCookie.js", at:"Foot" %}

    {% resources type: "HeadScript" %}
    {% resources type: "HeadLink" %}
    {% resources type: "Stylesheet" %}
</head>
<body id="page-top" dir="{{ Culture.Dir }}">
  <nav class="navbar navbar-expand-lg navbar-dark fixed-top" id="mainNav">
    <div class="container">
      <a class="navbar-brand js-scroll-trigger" href="{{ '~/#page-top' | href }}">{{ Site.SiteName }}</a>
      <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
        {{ "Menu" | t }}<i class="fas fa-bars ml-1"></i>
      </button>
      {% shape "menu", alias: "alias:main-menu", cache_id: "main-menu", cache_tag: "alias:main-menu" %}
    </div>
  </nav>
    {% render_section "Header", required: false %}
    <div class="container messages-wrapper">
        {% render_section "Messages", required: false %}
    </div>
  {% render_section "Content" %}
  {% render_section "Footer", required: false %}
  {% render_section "Modal", required: false %}
  {% resources type: "FootScript" %}
</body>
</html>
